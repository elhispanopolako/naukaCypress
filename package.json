{
    "name": "nauka-cypress-e2e",
    "scripts": {
        "start": "npx cypress open",
        "reportJunit": "npx cypress run --spec \"cypress/e2e/demoQA/**\" --reporter junit",
        "runTestAllure": "npx cypress run --spec \"cypress/e2e/demoQA/**\" --env allure=true",
        "allure:report": "allure generate allure-results --clean -o allure-report",
        "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
        "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true"
    },
    "devDependencies": {
        "@shelex/cypress-allure-plugin": "^2.33.1",
        "cy-verify-downloads": "^0.1.10",
        "cypress": "^10.3.0",
        "cypress-multi-reporters": "^1.6.1",
        "faker": "^6.6.6",
        "mocha-junit-reporter": "^2.0.2",
        "mysql": "^2.18.1"
    },
    "dependencies": {
        "@faker-js/faker": "^7.5.0",
        "fs": "^0.0.1-security",
        "node-xlsx": "^0.21.0",
        "path": "^0.12.7"
    }
}
