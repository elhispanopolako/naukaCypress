{
    "name": "nauka-cypress-e2e",
    "scripts": {
        "start": "npx cypress open",
        "start:swapi": "npx cypress open --env env=swapi",
        "start:demoqa": "npx cypress open --env env=demoQA",
        "start:swaglab": "npx cypress open --env env=swaglab",
        "report": "npx cypress open --env env=demoQA --spec \"cypress/e2e/demoQA/**\" --reporter junit"
    },
    "devDependencies": {
        "cypress-multi-reporters": "^1.6.1",
        "mocha-junit-reporter": "^2.0.2"
    }
}
